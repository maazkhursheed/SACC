    "ios": "react-native run-ios",
    "start": "react-native start",
    "android:staging": "react-native run-android --variant=sitDebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:dev": "react-native run-android --variant=uatdebug",
    "android:dev-release": "react-native run-android --variant=uatrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "ios:sit": "react-native run-ios --scheme 'SACC SIT'",
    "ios:prod": "react-native run-ios --scheme 'SACC'",
    "ios:uat": "react-native run-ios --scheme 'SACC UAT'",
    "android:build-prod": "cd android && ./gradlew assembleProdRelease",
    "android:build-sit": "cd android && ./gradlew assembleSitRelease",
    "android:build-uat": "cd android && ./gradlew assembleUatRelease",
    "android:bundle-prod": "cd android && ./gradlew bundleProdRelease",
    "android:bundle-sit": "cd android && ./gradlew bundleSitRelease",
